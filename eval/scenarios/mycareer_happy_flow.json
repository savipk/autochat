{
  "name": "mycareer_happy_flow",
  "description": "Happy path through MyCareer agent: suggest skills -> update profile -> find matches -> ask JD Q&A (x2) -> draft message -> send message -> apply for role",
  "agent": "mycareer",
  "profile_path": "data/miro_profile.json",
  "turns": [
    {
      "turn": 1,
      "user": "Suggest skills",
      "expectations": {
        "tool_called": "infer_skills",
        "response_contains": ["A2A", "MCP", "RAG"],
        "success": true
      }
    },
    {
      "turn": 2,
      "user": "Update profile with skills",
      "expectations": {
        "tool_called": "update_profile",
        "response_contains": ["updated", "skills"],
        "success": true
      }
    },
    {
      "turn": 3,
      "user": "Find matching roles for me",
      "expectations": {
        "tool_called": "get_matches",
        "response_contains": ["GenAI Lead"],
        "success": true
      }
    },
    {
      "turn": 4,
      "user": "What is the team size of the first role",
      "expectations": {
        "tool_called": "ask_jd_qa",
        "response_contains": ["10-15"],
        "success": true
      }
    },
    {
      "turn": 5,
      "user": "Which project is the first role focused on",
      "expectations": {
        "tool_called": "ask_jd_qa",
        "response_contains": ["AI applications"],
        "success": true
      }
    },
    {
      "turn": 6,
      "user": "Yes draft a message",
      "expectations": {
        "tool_called": "draft_message",
        "response_contains": ["Prasanth"],
        "success": true
      }
    },
    {
      "turn": 7,
      "user": "Send the message",
      "expectations": {
        "tool_called": "send_message",
        "success": true
      }
    },
    {
      "turn": 8,
      "user": "Apply for the role",
      "expectations": {
        "tool_called": "apply_for_role",
        "success": true
      }
    }
  ]
}
